{
    "manifest_version": 3,
    "name": "Reading Time",
    "version": 1.0,
    "description": "Adds the reading time to Chrome Extension",
    "icons": {
        "128": "Images/creeper.png"
    },
    "content_scripts": [
        {
            "js": ["Scripts/content.js"],
            "matches": [
                "https://developer.chrome.com/docs/extensions/*",
                "https://developer.chrome.com/docs/webstore/*"
            ]
        }
    ]
}